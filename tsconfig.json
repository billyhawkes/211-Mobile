{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@components/*": ["./src/components/*"],
            "@constants/*": ["./src/constants/*"],
            "@hooks/*": ["./src/hooks/*"],
            "@navigation/*": ["src/features/navigation/*"],
            "@assets/*": ["./src/assets/*"],
            "@screens/*": ["./src/screens/*"],
            "@typesGlobal/*": ["./src/types/*"],
            "@features/*": ["./src/features/*"]
        }
    },
    "include": ["src/**/*"]
}
